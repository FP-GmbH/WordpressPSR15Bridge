{
  "name": "fup/wordpress-middleware",
  "description": "This PSR-15 middleware allows you to include Wordpress into your Middleware-Pipeline",
  "minimum-stability": "dev",
  "license": "MIT",
  "prefer-stable": true,
  "authors": [
    {
      "name": "bastian charlet",
      "email": "bc@fp.de"
    }
  ],
  "autoload": {
    "psr-4": {
      "Fup\\WordpressMiddleware\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Fup\\WordpressMiddleware\\test\\": "test"
    }
  },
  "require": {
    "php": "^5.6 || ^7.2",
    "roave/security-advisories": "dev-master",
    "http-interop/http-middleware": "0.4.1",
    "zendframework/zend-diactoros": "1.6.1",
    "zendframework/zend-expressive-template": "1.0.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7.23 || 6.5.5"
  }
}
